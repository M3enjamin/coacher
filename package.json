{
  "name": "coacher",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --no-progress",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "build": "ng lint & ng build --prod --progress=false",
    "test": "ng test",
    "test:once": "ng test --watch=false",
    "bundle-report": "ng build -prod --stats-json & webpack-bundle-analyzer build/stats.json -m static",
    "webdriver:clean": "webdriver-manager clean",
    "webdriver:update": "webdriver-manager update --chrome",
    "webdriver:start": "webdriver-manager start --detach",
    "webdriver:stop": "webdriver-manager shutdown",
    "e2e": "webdriver-manager update --chrome && protractor protractor.conf.js",
    "ci": "yarn lint && yarn test:ci",
    "test:ci": "ng test --single-run --verbose",
    "deploy:staging": "yarn build && firebase use dev && firebase deploy",
    "deploy:prod": "yarn build && firebase use prod && firebase deploy",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update:check",
    "update:skip": "./node_modules/.bin/nx update:skip",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/cdk": "6.2.0",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/material": "6.2.0",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@angular/router": "6.0.0",
    "@ngrx/effects": "6.0.0",
    "@ngrx/router-store": "6.0.0",
    "@ngrx/store": "6.0.0",
    "@ngrx/store-devtools": "6.0.0",
    "@nrwl/nx": "6.0.0",
    "angularfire2": "5.0.0-rc.7",
    "core-js": "2.5.6",
    "firebase": "5.0.2",
    "hammerjs": "2.0.8",
    "lodash-es": "4.17.5",
    "mdi": "2.1.99",
    "ngrx-store-freeze": "0.2.3",
    "rxjs": "5.5.7",
    "zone.js": "git://github.com/JiaLiPassion/zone.js#jasmine-dist"
  },
  "devDependencies": {
    "@angular/cli": "6.0.2",
    "@angular/compiler-cli": "5.2.10",
    "@angular/language-service": "6.0.0",
    "@nrwl/schematics": "6.0.0",
    "@types/chai": "4.1.4",
    "@types/chai-as-promised": "7.1.0",
    "@types/cucumber": "4.0.1",
    "@types/jasmine": "2.8.7",
    "@types/jasminewd2": "2.0.2",
    "@types/node": "10.10.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-smoothie": "0.3.2",
    "codacy-coverage": "2.1.1",
    "codelyzer": "4.0.1",
    "cucumber": "4.0.0",
    "cucumber-html-report": "0.6.4",
    "cucumber-html-reporter": "4.0.1",
    "cucumberjs-allure-reporter": "1.0.3",
    "firebase-tools": "3.18.5",
    "husky": "0.14.3",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.3",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.4.3",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "1.1.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.13.0",
    "pretty-quick": "1.5.0",
    "protractor": "5.3.0",
    "protractor-cucumber-framework": "5.0.0",
