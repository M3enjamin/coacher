{
  "name": "coacher",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "build": "ng lint & ng build --prod",
    "test": "ng test",
    "test:once": "ng test --watch=false",
    "bundle-report": "ng build -prod --stats-json & webpack-bundle-analyzer build/stats.json -m static",
    "webdriver:clean": "webdriver-manager clean",
    "webdriver:update": "webdriver-manager update --chrome --ie --gecko",
    "webdriver:start": "webdriver-manager start --detach",
    "webdriver:stop": "webdriver-manager shutdown",
    "e2e": "protractor protractor.conf.js",
    "e2e:hub": "protractor protractor.hub.conf.js",
    "ci": "yarn lint && ng e2e",
    "test-with-coverage": "istanbul cover jasmine-node --captureExceptions spec/ && cat ./coverage/lcov.info | codacy-coverage && rm -rf ./coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.9",
    "@angular/cdk": "^5.1.0",
    "@angular/common": "5.2.9",
    "@angular/compiler": "5.2.9",
    "@angular/core": "5.2.9",
    "@angular/forms": "5.2.9",
    "@angular/http": "5.2.9",
    "@angular/material": "^5.1.0",
    "@angular/platform-browser": "5.2.9",
    "@angular/platform-browser-dynamic": "5.2.9",
    "@angular/router": "5.2.9",
    "@ngrx/effects": "^5.1.0",
    "@ngrx/router-store": "^5.0.1",
    "@ngrx/store": "^5.1.0",
    "@ngrx/store-devtools": "^5.1.0",
    "angularfire2": "^5.0.0-rc.4",
    "core-js": "^2.4.1",
    "firebase": "^4.8.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.5",
    "mdi": "^2.1.99",
    "ngrx-store-freeze": "^0.2.1",
    "rxjs": "^5.5.7",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "5.2.9",
    "@angular/language-service": "5.2.9",
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cucumber": "^4.0.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~9.6.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-smoothie": "^0.3.2",
    "codacy-coverage": "^2.1.1",
    "codelyzer": "^4.0.1",
    "cucumber": "^4.0.0",
    "cucumber-html-report": "^0.6.4",
    "cucumber-html-reporter": "^4.0.1",
    "cucumberjs-allure-reporter": "^1.0.3",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "protractor": "^5.3.0",
    "protractor-cucumber-framework": "^4.2.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "2.7.2",
    "yargs": "^11.0.0"
  }
}
